    "class Solution:\n",
    "    def sumOfMultiples(self, n: int) -> int:\n",
    "        nums = []\n",
    "        for i in range(1,n+1):\n",
    "            if i % 3 == 0 or i % 5 == 0 or i % 7 == 0:\n",
    "                nums.append(i)\n",
    "        return sum(nums)"
   ]
