   "source": [
    "class Solution:\n",
    "    def sumBase(self, n: int, k: int) -> int:\n",
    "        base_k = ''\n",
    "        while n > 0:\n",
    "            base_k = base_k + str(n % k)\n",
    "            n = int((n - (n % k))/k)\n",
    "        result = 0\n",
    "        for num in base_k:\n",
    "            result += int(num)\n",
    "        return result"
   ]
