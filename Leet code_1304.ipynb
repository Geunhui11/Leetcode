   "source": [
    "class Solution:\n",
    "    def sumZero(self, n: int) -> list[int]:\n",
    "        if n % 2 == 0:\n",
    "            return [int(-n/2) + i for i in range(0, n + 1) if i != int(n/2)]\n",
    "        else:\n",
    "            return [int(-(n-1)/2) + j for j in range(0, n)]"
   ]
