   "source": [
    "class Solution:\n",
    "    def generateTheString(self, n: int) -> str:\n",
    "        if n % 2 == 0:\n",
    "            return 'a'*(n-1) + 'b'\n",
    "        else:\n",
    "            return 'a'*n"
   ]
