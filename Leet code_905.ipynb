   "source": [
    "class Solution:\n",
    "    def sortArrayByParity(self, nums: List[int]) -> List[int]:\n",
    "        evens = []\n",
    "        odds = []\n",
    "        for num in nums:\n",
    "            if num % 2 == 0:\n",
    "                evens.append(num)\n",
    "            else:\n",
    "                odds.append(num)\n",
    "        return evens + odds"
   ]
  }
