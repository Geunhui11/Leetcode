    "class Solution:\n",
    "    def smallestEvenMultiple(self, n: int) -> int:\n",
    "        list_multiple = []\n",
    "        i = 1\n",
    "        while i<= 2*n:\n",
    "            if i % 2 == 0 and i % n == 0:\n",
    "                list_multiple.append(i)\n",
    "            i += 1\n",
    "        return min(list_multiple)"
   ]
  }
