   "source": [
    "class Solution:\n",
    "    def minMovesToSeat(self, seats: List[int], students: List[int]) -> int:\n",
    "        seats.sort()\n",
    "        students.sort()\n",
    "        count = 0\n",
    "        for i in range(len(seats)):\n",
    "            count += abs(seats[i] - students[i])\n",
    "        return count"
   ]
  }
