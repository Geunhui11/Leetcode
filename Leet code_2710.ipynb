   "source": [
    "class Solution:\n",
    "    def removeTrailingZeros(self, num: str) -> str:\n",
    "        temp = len(num)\n",
    "        for i in range(len(num)):\n",
    "            a = list(num[i:])\n",
    "            if all(digit == '0' for digit in a):\n",
    "                temp = i\n",
    "                break\n",
    "        return num[0:temp]"
   ]
  }
