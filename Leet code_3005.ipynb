    "class Solution:\n",
    "    def maxFrequencyElements(self, nums: List[int]) -> int:\n",
    "        num_set = set(nums)\n",
    "        count_list = []\n",
    "        for num in set(nums):\n",
    "            count_list.append(nums.count(num))\n",
    "        return max(count_list)*count_list.count(max(count_list))"
   ]
  }
