   "source": [
    "class Solution:\n",
    "    def reverseWords(self, s: str) -> str:\n",
    "        word_list = s.split()\n",
    "        for i in range(len(word_list)):\n",
    "            word_list[i] = word_list[i][::-1]\n",
    "        return ' '.join(word_list)"
   ]
  }
