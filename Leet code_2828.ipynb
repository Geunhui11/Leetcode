   "source": [
    "class Solution:\n",
    "    def isAcronym(self, words: List[str], s: str) -> bool:\n",
    "        result_str = ''\n",
    "        for word in words:\n",
    "            result_str += word[0]\n",
    "\n",
    "        return s == result_str"
   ]
  }
