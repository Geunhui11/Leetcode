   "source": [
    "class Solution:\n",
    "    def findPermutationDifference(self, s: str, t: str) -> int:\n",
    "        out = 0\n",
    "        for char in s:\n",
    "            out += abs(s.find(char) - t.find(char))\n",
    "        return out"
   ]
  }
