   "source": [
    "class Solution:\n",
    "    def firstPalindrome(self, words: List[str]) -> str:\n",
    "        output = ''\n",
    "        for i in range(len(words)):\n",
    "            if words[i] == words[i][::-1]:\n",
    "                output += words[i]\n",
    "                break\n",
    "        return output"
   ]
  }
