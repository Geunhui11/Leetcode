   "source": [
    "class Solution:\n",
    "    def restoreString(self, s: str, indices: List[int]) -> str:\n",
    "        shuffled = ['']*len(s)\n",
    "        for i, char in enumerate(s):\n",
    "            shuffled[indices[i]] = char\n",
    "        return ''.join(shuffled)"
   ]
  }
