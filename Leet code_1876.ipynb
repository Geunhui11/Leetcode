   "source": [
    "class Solution:\n",
    "    def countGoodSubstrings(self, s: str) -> int:\n",
    "        return len([s[i:i+3] for i in range(len(s)-2) if len(s[i:i+3]) == len(set(s[i:i+3]))])"
   ]
  }
