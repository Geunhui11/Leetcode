   "source": [
    "class Solution:\n",
    "    def repeatedNTimes(self, nums: List[int]) -> int:\n",
    "        S = set(nums)\n",
    "        for num in S:\n",
    "            if nums.count(num) == len(nums)//2:\n",
    "                return num"
   ]
