   "source": [
    "class Solution:\n",
    "    def returnToBoundaryCount(self, nums: List[int]) -> int:\n",
    "        count = 0\n",
    "        for i in range(1,len(nums)+1):\n",
    "            if sum(nums[0:i]) == 0:\n",
    "                count += 1\n",
    "        return count"
   ]
  }
