   "source": [
    "class Solution:\n",
    "    def totalMoney(self, n: int) -> int:\n",
    "        saving = [1, 2, 3, 4, 5, 6, 7]\n",
    "        nth_day_saving = 0\n",
    "        i = 0\n",
    "        while i < n:\n",
    "            j = i // 7\n",
    "            nth_day_saving += j + saving[i%7]\n",
    "            i += 1\n",
    "        return nth_day_saving"
   ]
  }
