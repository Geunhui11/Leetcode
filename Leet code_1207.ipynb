   "source": [
    "class Solution:\n",
    "    def uniqueOccurrences(self, arr: List[int]) -> bool:\n",
    "        S = set(arr)\n",
    "        lst = []\n",
    "        for num in S:\n",
    "            lst.append(arr.count(num))\n",
    "        if len(set(lst)) == len(lst):\n",
    "            return True\n",
    "        else:\n",
    "            return False"
   ]
  }
