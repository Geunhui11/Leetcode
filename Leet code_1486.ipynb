   "source": [
    "class Solution:\n",
    "    def xorOperation(self, n: int, start: int) -> int:\n",
    "        nums = [start + 2*i for i in range(n)]\n",
    "        xor_result = 0\n",
    "\n",
    "        for num in nums:\n",
    "            xor_result ^= num\n",
    "        \n",
    "        return xor_result"
   ]
  }
