   "source": [
    "class Solution:\n",
    "    def sortSentence(self, s: str) -> str:\n",
    "        list_word = s.split()\n",
    "        arr = [i[-1] + i[:-1] for i in list_word]\n",
    "        arr.sort()\n",
    "        ans = ''\n",
    "        for i in arr:\n",
    "            ans += i[1:] + ' '\n",
    "        \n",
    "        return ans[:-1]"
   ]
  }
