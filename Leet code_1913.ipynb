   "source": [
    "class Solution:\n",
    "    def maxProductDifference(self, nums: List[int]) -> int:\n",
    "        nums.sort()\n",
    "        x = nums[-1]\n",
    "        y = nums[-2]\n",
    "        z = nums[0]\n",
    "        w = nums[1]\n",
    "        return x*y - z*w"
   ]
  }
