   "source": [
    "class Solution:\n",
    "    def isSameAfterReversals(self, num: int) -> bool:\n",
    "        if str(num) == '0':\n",
    "            return True\n",
    "        else:\n",
    "            if str(num)[-1] == '0':\n",
    "                return False\n",
    "            else:\n",
    "                return True"
   ]
