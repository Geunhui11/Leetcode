   "source": [
    "class Solution:\n",
    "    def countKDifference(self, nums: List[int], k: int) -> int:\n",
    "        count = 0\n",
    "        for i in range(len(nums)-1):\n",
    "            for j in range(i,len(nums)):\n",
    "                if abs(nums[i] - nums[j]) == k:\n",
    "                    count += 1\n",
    "        return count"
   ]
  }
